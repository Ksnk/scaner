{
    "name": "ksnk/scaner",
    "description": "Scaner utilities",
    "keywords": ["ksnk", "scaer"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^5.3",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.14",
        "codeception/codeception": "2.3.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Ksnk\\testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
