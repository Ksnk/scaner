{
    "name": "ksnk/scaner",
    "description": "Scaner utilities",
    "keywords": ["ksnk", "scaner"],
    "license": "MIT",
    "type": "project",
    "require-dev": {
        "google/apiclient": "1.1.7",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Ksnk\\testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "phpoffice/phpspreadsheet": "1.8.0"
    }
}
